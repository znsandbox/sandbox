<!DOCTYPE html>
{{ style('/bundles/web/vendor/fontawesome-free/css/all.min.css') }}
{{ style('/bundles/web/vendor/adminlte/css/adminlte.min.css') }}
{{ style('https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css') }}
{{ style('https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700') }}
{{ script('/bundles/web/vendor/jquery/jquery.min.js') }}
{{ script('/bundles/web/vendor/bootstrap/js/bootstrap.bundle.min.js') }}
{{ script('/bundles/web/vendor/adminlte/js/adminlte.min.js') }}
{{ script('/bundles/web/vendor/adminlte/js/demo.js') }}
{{ breadcrumbAdd('<i class="fa fa-home" aria-hidden="true"></i>', '/admin') }}
{% set bodyCode %}
    {% block body %}<i class="text-muted">Empty content</i>{% endblock %}
{% endset %}
<html>
<head>
    <meta charset="utf-8">
    <title>{% block title %}Admin panel{% endblock %}</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% include '@Notify/notify.html.twig' %}
    {% include '@Web/style.html.twig' %}
</head>
<body class="hold-transition sidebar-mini">
<!-- Site wrapper -->
<div class="wrapper">
    <!-- Navbar -->
    <nav class="main-header navbar navbar-expand navbar-white navbar-light">
        {% include '@Admin/block/navbar.html.twig' %}
    </nav>
    <!-- /.navbar -->

    <!-- Main Sidebar Container -->
    <aside class="main-sidebar sidebar-dark-primary elevation-4">
        {% include '@Admin/block/main_sidebar.html.twig' %}
    </aside>

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <div class="container-fluid">
                <div class="row mb-2">
                    <div class="col-sm-6">
                        <h1>{{ block('title') }}</h1>
                    </div>
                    <div class="col-sm-6">
                        <div class="float-sm-right">
                            {{ breadcrumb() }}
                        </div>
                    </div>
                </div>
            </div><!-- /.container-fluid -->
        </section>

        <!-- Main content -->
        <section class="content">
            {{ bodyCode }}
        </section>
        <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->

    <footer class="main-footer">
        {% include '@Admin/block/footer.html.twig' %}
    </footer>

    <!-- Control Sidebar -->
    <aside class="control-sidebar control-sidebar-dark">
        {% include '@Admin/block/control_sidebar.html.twig' %}
    </aside>
    <!-- /.control-sidebar -->
</div>
<!-- ./wrapper -->

{% include '@Web/script.html.twig' %}
</body>
</html>
